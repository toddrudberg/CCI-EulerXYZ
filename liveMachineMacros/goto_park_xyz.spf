PROC GOTO_PARK_XYZ DISPLOF

DEF REAL XPOS  = 0.0
DEF REAL YPOS  = 1200.0
DEF REAL ZPOS  = 600.0
DEF REAL RXPOS = 0.0
DEF REAL RYPOS = 0.0
DEF REAL RZPOS = 0.0
DEF REAL F_PTP = 1000.0
DEF INT INITWOFF

INITWOFF=$P_GG[8]
STOPRE
G500

RETRACT_XYZ

PTP G1 X=XPOS Y=YPOS Z=ZPOS RX=RXPOS RY=RYPOS RZ=RZPOS F=F_PTP
CP
STOPRE

G[8]=INITWOFF

RET

